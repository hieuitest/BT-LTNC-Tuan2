#include<bits/stdc++.h>

using namespace std;

int main()
{
    int n;
    cin >> n;
    for ( int i = 0 ; i < n ; i++)
    {
        for ( int j = i + 1 ; j <= n ; j++)
        {
            cout << j << " " ;
            if (j == n && i > 0 )
            {
                for (int k = 1 ; k <= i ; k++)
                {
                    cout << k << " ";
                }
            }
        }
        cout << endl;
    }
    return 0;
}
